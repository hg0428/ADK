class Test {
	~init(x, y) {
		this.x=x
		this.y=y
	}
  
	funct setPos(x, y) {
		this.x=x
		this.y=y
	}
  
	funct moveX(amount) {
    this.x+=amount

    output(this.x)
  }
	funct moveY(amount) {
    this.y+=amount

    output(this.y)
  }
	funct print() {
		output("X:", this.x)
		output("Y:", this.y)
	}
}

output()

a=Test(0,0)
output(a)
a.setPos(8,9)
a.moveY(-5)
a.print() // X: 8 --\n--Y: 4

