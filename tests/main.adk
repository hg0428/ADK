class Test {
	~init(x, y) {
		this.x=x
		this.y=y
	}
  
	funct setPos(x, y) {
		this.x=x
		this.y=y
	}
  
	funct moveX(amount) {this.x+=amount}
	funct moveY(amount) {this.y+=amount}
	funct print() {
		output("X:", this.x)
		output("Y:", this.y)
	}
}

